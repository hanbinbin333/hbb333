        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>if OBJECT_ID('tempdb..#tempJobRunInf') is not null
 drop table #tempJobRunInf
SELECT 
			CAST(SERVERPROPERTY(N'SERVERNAME') AS sysname) AS [Instance],
			J.[Name] [JobName],   
			CASE J.[enabled] WHEN 0 THEN '禁用' WHEN 1 THEN '启用' ELSE '' END [Enabled], 
			CASE JS.[last_run_outcome] WHEN 0 THEN '失败'  WHEN 1 THEN '成功' WHEN 2 THEN '取消' WHEN 5 THEN '未知' ELSE '' END [LastRunResult], 
			JS.[Last_Run_Duration],
			CAST(STUFF(STUFF(CAST(JS.[Last_Run_Date] AS varchar),5,0,'-'),8,0,'-') + ' ' + STUFF(STUFF(REPLACE(STR(JS.last_run_time,6,0),' ','0'),3,0,':'),6,0,':') AS DATETIME) [LastRunDT] 
			into #tempJobRunInf
	FROM				msdb.dbo.sysjobs J 
		LEFT JOIN msdb.dbo.sysjobservers JS On J.job_id = JS.job_id 
		LEFT JOIN msdb.dbo.sysdbmaintplan_jobs PJ On J.job_id = PJ.job_id
		LEFT JOIN msdb.dbo.sysdbmaintplans P On P.plan_id = PJ.plan_id
	WHERE J.[name] &lt;&gt; 'syspolicy_purge_history'
	order by  jobName

select JobName,
       LastRunResult,
       LastRunDT
  from #tempJobRunInf
  where Enabled='启用'
    and LastRunResult ='失败'</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
  </DataSets>
